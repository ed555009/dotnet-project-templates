{
	"Serilog": {
		"MinimumLevel": {
			"Default": "Debug",
			"Override": {
				"Microsoft": "Warning",
				"System": "Warning",
				"Microsoft.Hosting.Lifetime": "Information",
				"Microsoft.EntityFrameworkCore": "Warning"
			}
		},
		"Using": [
			"Serilog.Expressions"
		],
		"Enrich": [
			"FromLogContext",
			"WithMachineName",
			"WithProcessId",
			"WithThreadId"
		]
	},
	"AllowedHosts": "*"
}